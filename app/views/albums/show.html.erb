<p>
  <b>Title:</b>
  <%=h @album.title %>
</p>

<p>
  <b>Description:</b>
  <%=h @album.description %>
</p>

<% @album.photos.each do |photo| %>
      <%= link_to(image_tag(photo.photo.url(:thumb)), album_photo_path(@album, photo))  %>

<% end %>
<p>

  <%= link_to 'Add Photo', new_album_photo_path(@album) %>
</p>


<%= link_to 'Edit', edit_album_path(@album) %> |
<%= link_to 'Back', albums_path %>
