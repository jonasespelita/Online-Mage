<h1>New photo</h1>

<% form_for([:album, @photo], :html => { :multipart => true } ) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label "Select Photo"%><br />
    <%= f.file_field :photo%>
  </p>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description,  :rows => 5 %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back',  album_photos_path(@album) %>
